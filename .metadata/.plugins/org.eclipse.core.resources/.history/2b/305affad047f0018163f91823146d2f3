
public class Exam4 {
	public static void main(String args[]) { 
		question04();
	}
	
	public static void question04() {
		//4 Predict the output?
		int arr[][] = new int[4][]; 
		
		arr[0] = new int[1]; 
		arr[1] = new int[2]; 
		arr[2] = new int[3]; 
		arr[3] = new int[4]; 
		
		int i, j, k = 0;
		
		for(i = 0; i < arr.length; i++) {
			for(j = 0; j < arr[i].length; j++) {
				arr[i][j] = i*j;
				System.out.print(arr[i][j]);
			}
			System.out.println();
		}
	}

}
